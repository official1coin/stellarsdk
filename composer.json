{
  "name": "official1coin/stellarsdk",
  "type": "library",
  "version": "v1.0.8",
  "description": "API client for the Stellar network",
  "autoload": {
    "psr-4": {
      "OneCoin\\StellarSdk\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^8.0",
    "christian-riesen/base32": "^1.6",
    "guzzlehttp/guzzle": "^7.0",
    "laravel/framework": "^8.33",
    "paragonie/sodium_compat": "^1.16",
    "phpseclib/phpseclib": "^3.0",
    "symfony/filesystem": "^5.2"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Aaron Shames",
      "email": "aaron@1coin.io"
    }
  ],
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "OneCoin\\StellarSdk\\StellarSdkServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "phpunit/phpunit": "^9.3.3"
  }
}
